  </joint>

  <joint name="rl_knee_joint" type="revolute">
    <parent link="rl_thigh_link"/>
    <child link="rl_shank_link"/>
    <origin xyz="0 0 -0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
