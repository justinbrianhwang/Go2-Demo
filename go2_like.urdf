      </geometry>
    </collision>
  </link>

  <link name="rr_shank_link">
    <inertial>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <mass value="0.2"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001" ixy="0" ixz="0" iyz="0"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.04 0.04 0.2"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 -0.1" rpy="0 0 0"/>
      <geometry>
        <box size="0.04 0.04 0.2"/>
      </geometry>
    </collision>
  </link>

  <!-- Joints: hip (roll), thigh (pitch), knee (pitch) for each leg -->

  <!-- Front Left -->
  <joint name="fl_hip_roll_joint" type="revolute">
    <parent link="base_link"/>
    <child link="fl_hip_link"/>
    <origin xyz="0.2 0.1 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.6" upper="0.6" effort="20" velocity="5"/>
  </joint>

  <joint name="fl_hip_pitch_joint" type="revolute">
    <parent link="fl_hip_link"/>
    <child link="fl_thigh_link"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.0" upper="1.0" effort="20" velocity="5"/>
  </joint>

  <joint name="fl_knee_joint" type="revolute">
    <parent link="fl_thigh_link"/>
    <child link="fl_shank_link"/>
    <origin xyz="0 0 -0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="0.5" effort="20" velocity="5"/>
  </joint>

  <!-- Front Right -->
  <joint name="fr_hip_roll_joint" type="revolute">
    <parent link="base_link"/>
    <child link="fr_hip_link"/>
    <origin xyz="0.2 -0.1 0.0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.6" upper="0.6" effort="20" velocity="5"/>
